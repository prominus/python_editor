{
	"compilerOptions": {
		"baseUrl": ".",
	  "target": "es6",
	  "module": "commonjs",
	  "moduleResolution": "node",
	  "sourceMap": true,
	  "declarationMap": true,
	  "inlineSources": false,
	  "declaration": true,
	  "stripInternal": true,
	  "lib": [
		"es2016",
		"dom"
	  ],
	  "rootDir": "src",
	  "outDir": "lib",
	  "strict": true,
	  "noImplicitReturns": true,
	  "noUnusedLocals": true,
	  "skipLibCheck": true
	},
	"include": [
	  "src"
	]
  }